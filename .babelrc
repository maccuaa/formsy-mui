{
  "presets": [
    "es2015",
    "react",
    "stage-3"
  ],
  "plugins": [
    "transform-export-extensions"
  ],
  "env": {
    "test": {
      "sourceMaps": "both"
    },
    "release": {
      "plugins": [
        "transform-runtime",
        ["transform-react-remove-prop-types", {"mode": "wrap"}]
      ]
    }
  }
}
